{
  "manifest_version": 3,
  "name": "ProofIt!",
  "description": "an internal VCS tool, that facilitate generating a pdf proof for proshop templates!",
  "version": "1.0.1",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_icon": "icon.png",
    "default_title": "ProofIt!",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["contextMenus", "tabs"]



}
